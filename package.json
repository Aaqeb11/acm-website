{
	"name": "acm-website",
	"scripts": {
		"generate:css": "unocss 'src/**/*.tsx' --out-file ./src/styles/unocss.css",
		"dev": "concurrently 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "NODE_ENV=production node cpanel.js",
		"pro:start": "next start -p 3080",
		"lint": "next lint",
		"format": "prettier --cache --write src"
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@mantine/core": "^5.6.4",
		"@mantine/hooks": "^5.6.4",
		"framer-motion": "^7.6.4",
		"next": "^13.0.1",
		"nextjs-progressbar": "^0.0.16",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.6.0"
	},
	"packageManager": "yarn@3.2.4",
	"devDependencies": {
		"@types/eslint": "^8",
		"@types/node": "18.11.9",
		"@types/prettier": "^2",
		"@types/react": "18.0.24",
		"@unocss/cli": "^0.46.3",
		"@unocss/reset": "^0.46.3",
		"concurrently": "^7.5.0",
		"eslint": "^8.26.0",
		"eslint-config-mahir": "^0.0.13",
		"prettier": "^2.7.1",
		"typescript": "4.8.4",
		"unocss": "^0.46.3"
	}
}
